<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.module.category" id="module_content_generation">
        <field name="name">Content Generation</field>
        <field name="description">Category for Content Generation</field>
    </record>
    <record id="group_content_generation_product_generator" model="res.groups">
        <field name="name">Product Generator</field>
        <field name="category_id" ref="module_content_generation"/>
    </record>
    <record id="group_content_generation_set_generator" model="res.groups">
        <field name="name">Set Generator</field>
        <field name="category_id" ref="module_content_generation"/>
        <field name="implied_ids" eval="[(4, ref('group_content_generation_product_generator'))]"/>
    </record>
</odoo>